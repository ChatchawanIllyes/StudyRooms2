{
  "expo": {
    "name": "StudyApp",
    "slug": "study-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.studyapp.mobile",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.studyapp.mobile",
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "WAKE_LOCK"
      ]
    },
    "extra": {
      "eas": {
        "projectId": "ff925315-a2cc-40e4-a46b-5b4da6a22d3c"
      }
    },
    "plugins": [
      "expo-asset",
      "expo-font"
    ]
  }
}
